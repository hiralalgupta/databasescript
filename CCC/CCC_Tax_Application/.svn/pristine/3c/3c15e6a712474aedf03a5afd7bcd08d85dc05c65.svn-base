        <version>2.19.1</version>
        <configuration>
          <showSuccess>false</showSuccess>
        </configuration>
      </plugin>
       <plugin>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>3.6</version>
        <reportSets>
          <reportSet>
            <reports>
              <report>pmd</report>
              <report>cpd</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
    </plugins>
  </reporting>
